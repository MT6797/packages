<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="@string/settings">
    <Preference
        android:key="apn_settings"
        android:title="@string/apn_settings"
        android:summary="@string/apn_settings_summary"
        android:persistent="false"
        />
    <CheckBoxPreference
        android:key="enable_http_proxy"
        android:title="@string/enable_http_proxy"
        android:summary="@string/enable_http_proxy_summary"
        android:persistent="false"
        />
    <Preference
        android:key="http_proxy_settings"
        android:dependency="enable_http_proxy"
        android:title="@string/http_proxy_settings"
        android:summary="@string/http_proxy_settings_summary"
        android:persistent="false"
        />
    <Preference
        android:key="http_buffer_size"
        android:title="@string/http_buffer_size"
        android:summary="@string/http_buffer_size_summary"
        android:persistent="false"
        />
    <CheckBoxPreference
        android:key="enable_rtsp_proxy"
        android:title="@string/enable_rtsp_proxy"
        android:summary="@string/enable_rtsp_proxy_summary"
        android:persistent="false"
        />
    <Preference
        android:key="rtsp_proxy_settings"
        android:dependency="enable_rtsp_proxy"
        android:title="@string/rtsp_proxy_settings"
        android:summary="@string/rtsp_proxy_settings_summary"
        android:persistent="false"
        />
    <Preference
        android:key="udp_port_settings"
        android:title="@string/udp_port_settings"
        android:summary="@string/udp_port_settings_summary"
        android:persistent="false"
        />
     <Preference
        android:key="rtsp_buffer_size"
        android:title="@string/rtsp_buffer_size"
        android:summary="@string/rtsp_buffer_size_summary"
        android:persistent="false"
        />
</PreferenceScreen>
